files:
  "/opt/elasticbeanstalk/tasks/bundlelogs.d/irisprod.conf":
    content: |
      /tmp/irisprod*

  "/opt/elasticbeanstalk/tasks/taillogs.d/irisprod.conf":
    content: |
      /tmp/irisprod.log

option_settings:
  aws:elasticbeanstalk:cloudwatch:logs:
    StreamLogs: true
    DeleteOnTerminate: false
    RetentionInDays: 180
